{
    "name": "elevenmarianne/intelligence-artifi-hair",
    "description": "Demo AI",
    "type": "project",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Marianne"
        }
    ],
    "require": {
        "php": "^8.4",
        "symfony/http-client": "^7.3",
        "ramsey/uuid": "^4.9",
        "symfony/console": "^7.3",
        "symfony/ai-platform": "dev-main",
        "symfony/flex": "^2.8",
        "symfony/asset": "^7.3",
        "symfony/monolog-bundle": "^3.10",
        "symfony/mailer": "^7.3",
        "symfony/form": "^7.3",
        "symfony/security-bundle": "^7.2",
        "symfony/translation": "^7.3",
        "symfony/validator": "^7.3",
        "doctrine/dbal": "^3",
        "doctrine/doctrine-bundle": "^2.16",
        "doctrine/doctrine-migrations-bundle": "^3.4",
        "doctrine/orm": "^3.5",
        "symfony/twig-bundle": "^7.2",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0",
        "symfony/ai-store": "dev-main",
        "symfony/runtime": "^7.3",
        "symfony/yaml": "^7.3",
        "symfony/ai-bundle": "dev-main"
    },
    "auto-scripts": {
        "cache:clear": "symfony-cmd",
        "assets:install public": "symfony-cmd"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "require-dev": {
        "symfony/dotenv": "^7.3",
        "symfony/maker-bundle": "^1.64",
        "doctrine/doctrine-fixtures-bundle": "^4.1",
        "symfony/stopwatch": "^7.3",
        "symfony/web-profiler-bundle": "^7.2"
    }
}
